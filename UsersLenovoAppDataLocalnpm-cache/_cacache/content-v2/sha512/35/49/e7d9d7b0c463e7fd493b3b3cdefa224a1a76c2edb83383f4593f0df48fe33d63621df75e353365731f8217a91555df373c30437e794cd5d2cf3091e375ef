getProjectConfig({platformName: 'visionos'}),\n      dependencyConfig: getDependencyConfg({platformName: 'visionos'}),\n    },\n    ..\n  },\n```\n","gitHead":"148f4d7220fba7a55e1f0b7def417c6c96ea6d9c","_npmUser":{"name":"thymikee","email":"thymikee@gmail.com"},"repository":{"url":"git+https://github.com/react-native-community/cli.git","type":"git","directory":"packages/cli-platform-apple"},"_npmVersion":"lerna/7.1.5/node@v20.8.0+arm64 (darwin)","description":"This package is part of the [React Native CLI](../../README.md). It contains utilities for building reusable commands targetting Apple platforms.","directories":{},"_nodeVersion":"20.8.0","dependencies":{"ora":"^5.4.1","glob":"^7.1.3","chalk":"^4.1.2","execa":"^5.0.0","fast-xml-parser":"^4.0.12","@react-native-community/cli-tools":"13.3.0"},"publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"hasbin":"^1.2.3","@types/glob":"^7.1.1","@types/lodash":"^4.14.149","@react-native-community/cli-types":"13.3.0"},"_npmOperationalInternal":{"tmp":"tmp/cli-platform-apple_13.3.0_1704714369194_0.8956645860269126","host":"s3://npm-registry-packages"}},"13.4.0":{"name":"@react-native-community/cli-platform-apple","version":"13.4.0","license":"MIT","_id":"@react-native-community/cli-platform-apple@13.4.0","maintainers":[{"name":"dmitryrykun","email":"dmitry.rykun@gmail.com"},{"name":"safaiyeh","email":"safaiyeh@protonmail.com"},{"name":"cortinico","email":"corti.nico@gmail.com"},{"name":"lunaleaps","email":"lunaleaps@gmail.com"},{"name":"bartoszklonowski","email":"bartosz.klonowski@cxcode.pl"},{"name":"titozzz","email":"malbranche.thibault@gmail.com"},{"name":"empyrical","email":"empyrical@outlook.com"},{"name":"cpojer","email":"christoph.pojer@gmail.com"},{"name":"mattoakes","email":"hello@mattoakes.net"},{"name":"ferrannp","email":"fnp.developer@gmail.com"},{"name":"mike866","email":"michal.chudziak@gmail.com"},{"name":"krizzu","email":"krizzu.dev@gmail.com"},{"name":"t