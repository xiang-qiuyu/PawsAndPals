g: true` in your app/build.gradle file, inside the `project.ext.react` map.\n","gitHead":"54d48a4e08a1aef334ae6168788e0157a666b4f5","_npmUser":{"name":"thymikee","email":"thymikee@gmail.com"},"repository":{"url":"git+https://github.com/react-native-community/cli.git","type":"git","directory":"packages/cli-hermes"},"_npmVersion":"lerna/7.1.5/node@v20.8.0+arm64 (darwin)","description":"This package is part of the [React Native CLI](../../README.md). It contains commands for managing the Hermes engine.","directories":{},"_nodeVersion":"20.8.0","dependencies":{"chalk":"^4.1.2","hermes-profile-transformer":"^0.0.6","@react-native-community/cli-tools":"13.6.1","@react-native-community/cli-platform-android":"13.6.1"},"publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"@types/ip":"^1.1.0","@react-native-community/cli-types":"13.6.1"},"_npmOperationalInternal":{"tmp":"tmp/cli-hermes_13.6.1_1708619230226_0.8849954221374283","host":"s3://npm-registry-packages"}},"13.6.2":{"name":"@react-native-community/cli-hermes","version":"13.6.2","license":"MIT","_id":"@react-native-community/cli-hermes@13.6.2","maintainers":[{"name":"dmitryrykun","email":"dmitry.rykun@gmail.com"},{"name":"safaiyeh","email":"safaiyeh@protonmail.com"},{"name":"cortinico","email":"corti.nico@gmail.com"},{"name":"lunaleaps","email":"lunaleaps@gmail.com"},{"name":"bartoszklonowski","email":"bartosz.klonowski@cxcode.pl"},{"name":"titozzz","email":"malbranche.thibault@gmail.com"},{"name":"empyrical","email":"empyrical@outlook.com"},{"name":"cpojer","email":"christoph.pojer@gmail.com"},{"name":"mattoakes","email":"hello@mattoakes.net"},{"name":"ferrannp","email":"fnp.developer@gmail.com"},{"name":"mike866","email":"michal.chudziak@gmail.com"},{"name":"krizzu","email":"krizzu.dev@gmail.com"},{"name":"trensik","email":"dawid.urbaniak.dev@gmail.com"},{"name":"thymikee","email":"thymikee@gmail.com"},{"name":"nicholaslee119","email":"nicholaslee119@gmail.com"},{"