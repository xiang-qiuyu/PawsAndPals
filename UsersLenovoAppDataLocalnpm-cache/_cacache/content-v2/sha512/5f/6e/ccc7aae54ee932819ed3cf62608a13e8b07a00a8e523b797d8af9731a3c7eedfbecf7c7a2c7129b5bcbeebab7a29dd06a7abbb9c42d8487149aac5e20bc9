le file, inside the `project.ext.react` map.\n","gitHead":"bd0963dbe23b079b50dd2b07f80f305b3bc92f3a","_npmUser":{"name":"thymikee","email":"thymikee@gmail.com"},"repository":{"url":"git+https://github.com/react-native-community/cli.git","type":"git","directory":"packages/cli-hermes"},"_npmVersion":"lerna/4.0.0/node@v16.19.1+arm64 (darwin)","description":"This package is part of the [React Native CLI](../../README.md). It contains commands for managing the Hermes engine.","directories":{},"_nodeVersion":"16.19.1","dependencies":{"ip":"^1.1.5","chalk":"^4.1.2","hermes-profile-transformer":"^0.0.6","@react-native-community/cli-tools":"12.0.0-alpha.4","@react-native-community/cli-platform-android":"12.0.0-alpha.4"},"publishConfig":{"access":"public"},"_hasShrinkwrap":false,"readmeFilename":"README.md","devDependencies":{"@types/ip":"^1.1.0","@react-native-community/cli-types":"12.0.0-alpha.4"},"_npmOperationalInternal":{"tmp":"tmp/cli-hermes_12.0.0-alpha.4_1686571449373_0.6659201093274958","host":"s3://npm-registry-packages"}},"12.0.0-alpha.5":{"name":"@react-native-community/cli-hermes","version":"12.0.0-alpha.5","license":"MIT","_id":"@react-native-community/cli-hermes@12.0.0-alpha.5","maintainers":[{"name":"dmitryrykun","email":"dmitry.rykun@gmail.com"},{"name":"safaiyeh","email":"safaiyeh@protonmail.com"},{"name":"cortinico","email":"corti.nico@gmail.com"},{"name":"lunaleaps","email":"lunaleaps@gmail.com"},{"name":"bartoszklonowski","email":"bartosz.klonowski@cxcode.pl"},{"name":"titozzz","email":"malbranche.thibault@gmail.com"},{"name":"empyrical","email":"empyrical@outlook.com"},{"name":"cpojer","email":"christoph.pojer@gmail.com"},{"name":"mattoakes","email":"hello@mattoakes.net"},{"name":"ferrannp","email":"fnp.developer@gmail.com"},{"name":"mike866","email":"michal.chudziak@gmail.com"},{"name":"krizzu","email":"krizzu.dev@gmail.com"},{"name":"trensik","email":"dawid.urbaniak.dev@gmail.com"},{"name":"thymikee","email":"thymikee@gmail.com"},{"name":"nicholaslee11